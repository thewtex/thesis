issn = {0001-4966},
journal = {The Journal of the Acoustical Society of America},
month = apr,
number = {4},
pages = {2431--2437},
publisher = {ACOUSTICAL SOC AMER AMER INST PHYSICS},
title = {{Group velocity, phase velocity, and dispersion in human calcaneus in vivo}},
type = {Article},
volume = {121},
year = {2007}
}
